{
    "filename": "examples/simple.yal",
    "header": "",
    "definitions": {
        "digit": "[0-9]",
        "letter": "[a-zA-Z_]",
        "id": "letter (letter | digit)*",
        "number": "digit+",
        "keyword": "'print' | 'return' | 'None' | 'True' | 'False'"
    },
    "rules": [
        [
            "[' ' '\\t']",
            "return None"
        ],
        [
            "'\\n'",
            "return EOL"
        ],
        [
            "'#' (_ # '\\n')*",
            "return None"
        ],
        [
            "keyword",
            "return KEYWORD"
        ],
        [
            "number",
            "return INT"
        ],
        [
            "'+'",
            "return PLUS"
        ],
        [
            "'-'",
            "return MINUS"
        ],
        [
            "'*'",
            "return TIMES"
        ],
        [
            "'**'",
            "return POWER"
        ],
        [
            "'/'",
            "return DIV"
        ],
        [
            "'//'",
            "return FLOOR_DIV"
        ],
        [
            "'('",
            "return LPAREN"
        ],
        [
            "')'",
            "return RPAREN"
        ],
        [
            "id",
            "return IDENTIFIER"
        ],
        [
            "eof",
            "raise('Fin de buffer')"
        ],
        [
            ".",
            "raise('Error l√©xico: Caracter no reconocido' + lxm)"
        ]
    ],
    "errors": []
}