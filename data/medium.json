{
    "filename": "examples/medium.yal",
    "header": "",
    "definitions": {
        "digit": "['0'-'9']",
        "letter": "['a'-'z' 'A'-'Z']",
        "id": "letter (letter | digit)*",
        "number": "digit+",
        "string": "'\"' (letter | digit | ' ' | [!-/])* '\"'"
    },
    "rules": [
        [
            "[' ' '\\t']",
            "return lexbuf"
        ],
        [
            "| '\\n'",
            "return EOL"
        ],
        [
            "| number",
            "return int(lxm)"
        ],
        [
            "| string",
            "return STRING"
        ],
        [
            "| '+'",
            "return PLUS"
        ],
        [
            "| '-'",
            "return MINUS"
        ],
        [
            "| '*'",
            "return TIMES"
        ],
        [
            "| '/'",
            "return DIV"
        ],
        [
            "| '('",
            "return LPAREN"
        ],
        [
            "| ')'",
            "return RPAREN"
        ],
        [
            "| id",
            "return IDENTIFIER"
        ],
        [
            "| '='",
            "return ASSIGN"
        ],
        [
            "| ';'",
            "return SEMICOLON"
        ],
        [
            "| eof",
            "raise('Fin de buffer')"
        ],
        [
            "| .",
            "raise('Error l√©xico: Caracter no reconocido'  + lxm)"
        ]
    ],
    "errors": []
}